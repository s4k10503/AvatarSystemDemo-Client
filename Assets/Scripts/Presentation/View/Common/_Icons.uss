.icon-button {
    width: 30px;
    height: 30px;
    background-color: transparent;
    border-width: 0;
    padding: 0;
    margin: 0;
}

.icon {
    width: 100%;
    height: 100%;
    -unity-background-scale-mode: scale-to-fit;
    color: #000000;
}